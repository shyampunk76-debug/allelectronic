{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/health", "dest": "/api/health.js" },
    { "src": "/api/repair-request", "dest": "/api/repair-request.js" },
    { "src": "/api/admin/login", "dest": "/api/admin/login.js" },
    { "src": "/api/admin/requests", "dest": "/api/admin/requests.js" },
    { "src": "/api/admin/repair-request", "dest": "/api/admin/repair-request.js" },
    { "src": "/api/admin/update-status", "dest": "/api/admin/update-status.js" },
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "^(?!/api/).*", "dest": "/index.html" }
  ]
}
